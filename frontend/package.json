{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "^10.1.0",
    "ethers": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1"
  },
  "scripts": {
    "start": "cross-env PORT=3001 react-scripts start",
    "start:user": "cross-env PORT=3001 react-scripts start",
    "start:dev": "cross-env PORT=3002 REACT_APP_ENTRY=developer react-scripts start",
    "start:developer": "cross-env PORT=4001 REACT_APP_ENTRY=developer react-scripts start",
    "start:both": "concurrently \"npm run start:user\" \"npm run start:developer\"",
    "build": "react-scripts build",
    "build:dev": "cross-env REACT_APP_ENTRY=developer react-scripts build",
    "build:developer": "cross-env REACT_APP_ENTRY=developer react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "concurrently": "^8.2.2"
  }
}
